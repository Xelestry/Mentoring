{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "dependencies": {
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    }
  },
  "nowarn": [
    "1591"
  ],
  "xmlDoc": true,
  "additionalArguments": [
    "/ruleset:./StyleCop.ruleset",
    "/additionalfile:./stylecop.json"
  ],
  "AllowedHosts": "*"
}
